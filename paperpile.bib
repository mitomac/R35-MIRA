@ARTICLE{Schneider2011-wi,
  title       = "Global histone {H4K20} trimethylation predicts cancer-specific
                 survival in patients with muscle-invasive bladder cancer",
  author      = "Schneider, A C and Heukamp, L C and Rogenhofer, S and Fechner,
