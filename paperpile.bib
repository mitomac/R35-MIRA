@ARTICLE{Salzler2013-ik,
  title       = "A sequence in the Drosophila {H3-H4} Promoter triggers histone
                 locus body assembly and biosynthesis of replication-coupled
                 histone {mRNAs}",
  author      = "Salzler, H R and Tatomer, D C and Malek, P Y and McDaniel, S L
