@ARTICLE{Bernstein2006-bx,
  title       = "A bivalent chromatin structure marks key developmental genes
                 in embryonic stem cells",
  author      = "Bernstein, Bradley E and Mikkelsen, Tarjei S and Xie, Xiaohui
