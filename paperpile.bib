@ARTICLE{Cayrou2011-qs,
  title       = "Genome-scale analysis of metazoan replication origins reveals
                 their organization in specific but flexible sites defined by
                 conserved features",
  author      = "Cayrou, Christelle and Coulombe, Philippe and Vigneron, Alice
                 and Stanojcic, Slavica and Ganier, Olivier and Peiffer,
